{
  "name": "rentalroi",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "type-check": "turbo run type-check",
    "clean": "turbo run clean && rm -rf node_modules",
    "db:link:dev": "supabase link --project-ref czdenl1ortsyxuoqvalp",
    "db:link:prod": "supabase link --project-ref mgpacftgrgcyvpjguoxs",
