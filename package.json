    "db:types": "supabase gen types typescript --linked > packages/database/src/types.ts",
    "db:migration": "supabase migration new"
  },
  "devDependencies": {
    "@turbo/gen": "^1.11.0",
    "prettier": "^3.1.0",
    "tsx": "^4.21.0",
    "turbo": "^1.11.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "pnpm@8.10.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  }
}
